<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_order_manage">

    <fragment
        android:id="@+id/navigation_order_manage"
        android:name="com.example.tao.ui.order_manage.OrderManageFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_order_manage" />

    <fragment
        android:id="@+id/navigation_account"
        android:name="com.example.tao.ui.account.AccountFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_account" />

    <fragment
        android:id="@+id/navigation_month_account"
        android:name="com.example.tao.ui.month_account.HistoryFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_month_account" />
